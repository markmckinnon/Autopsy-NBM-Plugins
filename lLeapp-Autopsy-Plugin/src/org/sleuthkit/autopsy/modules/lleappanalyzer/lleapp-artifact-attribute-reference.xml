    </FileName>

    <FileName filename="auth_log Failed Logins History.tsv" description="Auth Log Failed Logins">
        <ArtifactName artifactname="LLEAPP_FAILED_LOGINS" comment="Auth Log Failed Logins">
            <AttributeName attributename="LLEAPP_AUTH_TIMESTAMP" columnName="timestamp" required="yes" />
            <AttributeName attributename="TSK_HOST" columnName="host" required="yes" />
            <AttributeName attributename="LLEAPP_PROCESS" columnName="process" required="yes" />
            <AttributeName attributename="LLEAPP_MESSAGE" columnName="message" required="yes" />
            <AttributeName attributename="TSK_TEXT_FILE" columnName="sourcefile" required="yes" />        </ArtifactName>
    </FileName>
    
    <FileName filename="auth_log History.tsv" description="Auth Log History">
        <ArtifactName artifactname="LLEAPP_LOGIN_HISTORY" comment="Auth Log History">
            <AttributeName attributename="LLEAPP_AUTH_TIMESTAMP" columnName="timestamp" required="yes" />
            <AttributeName attributename="TSK_HOST" columnName="host" required="yes" />
            <AttributeName attributename="LLEAPP_PROCESS" columnName="process" required="yes" />
            <AttributeName attributename="LLEAPP_MESSAGE" columnName="message" required="yes" />
