    </FileName>
    
    <FileName filename="auth_log sudo History.tsv" description="Auth Log Sudo History">
        <ArtifactName artifactname="LLEAPP_SUDO_HISTORY" comment="Auth Log Sudo History">
            <AttributeName attributename="LLEAPP_AUTH_TIMESTAMP" columnName="timestamp" required="yes" />
            <AttributeName attributename="TSK_HOST" columnName="host" required="yes" />
            <AttributeName attributename="TSK_USER_NAME" columnName="user" required="yes" />
            <AttributeName attributename="LLEAPP_MESSAGE" columnName="message" required="yes" />
            <AttributeName attributename="LLEAPP_TERMINAL" columnName="terminal" required="yes" />
            <AttributeName attributename="LLEAPP_WORKING_DIRECTORY" columnName="print_working_directory" required="yes" />
            <AttributeName attributename="LLEAPP_RUN_AS" columnName="run_as" required="yes" />
            <AttributeName attributename="LLEAPP_CMD_LINE" columnName="command" required="yes" />
            <AttributeName attributename="TSK_TEXT_FILE" columnName="sourcefile" required="yes" />        </ArtifactName>
    </FileName>
    
</lLeap_Files_To_Process>

