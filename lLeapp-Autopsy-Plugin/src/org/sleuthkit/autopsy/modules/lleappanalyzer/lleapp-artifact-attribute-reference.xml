            <AttributeName attributename="LLEAPP_REQUEST" columnName="request_method" required="yes" />
            <AttributeName attributename="TSK_COUNT" columnName="num_of_accesses" required="yes" />
            <AttributeName attributename="TSK_TEXT_FILE" columnName="sourcefile" required="yes" />        </ArtifactName>
    </FileName>

    <FileName filename="apache_uri_bytes History.tsv" description="Apache Url Bytes History">
        <ArtifactName artifactname="LLEAPP_URL_BYTES_HISTORY" comment="Apache Url Bytes History">
            <AttributeName attributename="TSK_URL" columnName="uri" required="yes" />
            <AttributeName attributename="LLEAPP_BYTES" columnName="num_of_bytes" required="yes" />
            <AttributeName attributename="TSK_TEXT_FILE" columnName="sourcefile" required="yes" />        </ArtifactName>
    </FileName>

    <FileName filename="apt_history_log History.tsv" description="Apt Log History">
        <ArtifactName artifactname="LLEAPP_APT_HISTORY_LOG" comment="Apt Log History">
            <AttributeName attributename="TSK_DATETIME_START" columnName="start_date" required="yes" />
            <AttributeName attributename="LLEAPP_CMD_LINE" columnName="commandline" required="yes" />
            <AttributeName attributename="TSK_ACTIVITY_TYPE" columnName="type" required="yes" />
            <AttributeName attributename="TSK_PROG_NAME" columnName="programs" required="yes" />
            <AttributeName attributename="TSK_DATETIME_END" columnName="end_date" required="yes" />
            <AttributeName attributename="null" columnName="start_date_epoch" required="no" />
            <AttributeName attributename="TSK_TEXT_FILE" columnName="sourcefile" required="yes" />        </ArtifactName>
    </FileName>
    
    <FileName filename="apt_programs_installed_log History.tsv" description="Apt Programs Installed">
        <ArtifactName artifactname="TSK_INSTALLED_PROG" comment="Apt Programs Installed">
            <AttributeName attributename="TSK_PROG_NAME" columnName="installed_program" required="yes" />
            <AttributeName attributename="TSK_DATETIME" columnName="date_installed" required="yes" />
            <AttributeName attributename="null" columnName="date_installed_epoch" required="no" />
            <AttributeName attributename="TSK_TEXT_FILE" columnName="sourcefile" required="yes" />        </ArtifactName>
    </FileName>

    <FileName filename="apt_programs_removed_log History.tsv" description="Apt Programs Removed">
